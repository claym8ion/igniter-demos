{% extends 'SIStoreBundle::index.html.twig' %}

{% block content %}
    {% if status_code == 404 %}
    <h1>Page Not Found</h1>
    <p>Whoops! We weren't able to find the page you were looking for.</p>
    <p>If you followed a link it may be out of date, or you may have mistyped the address.</p>
    {% else %}
    <h1>Internal Error</h1>
    <p>Uh oh, something went wrong.</p>
    {% endif %}
{% endblock %}